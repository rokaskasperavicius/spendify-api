{
  "name": "server",
  "version": "2.0.0",
  "description": "Spendify Server",
  "main": "index.js",
  "scripts": {
    "start": "ts-node --transpileOnly -r tsconfig-paths/register src/index.ts",
    "dev": "npm run codegen && ts-node-dev --watch src/openapi/openapi.yaml -r tsconfig-paths/register src/index.ts ",
    "prisma-migrate": "npx prisma migrate dev",
    "codegen": "openapi-typescript https://bankaccountdata.gocardless.com/api/v2/swagger.json -o ./src/services/gocardless/generated.d.ts",
    "eslint": "eslint . --max-warnings 0",
    "tsc": "tsc --noEmit",
    "ci": "npm run eslint && npm run tsc"
  },
  "engines": {
    "node": "22.14.0"
  },
  "prisma": {
    "seed": "ts-node --transpileOnly -r tsconfig-paths/register prisma/seed.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^1.13.0",
    "@prisma/client": "^6.7.0",
    "@types/express-serve-static-core": "^5.0.6",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "currency.js": "^2.0.4",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "fuzzysort": "^3.1.0",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "msw": "^2.7.5",
    "node-schedule": "^2.1.1",
    "password-validator": "^5.3.0",
    "request-ip": "^3.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^11.1.0",
    "yamljs": "^0.3.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.3",
    "@types/node-schedule": "^2.1.7",
    "@types/request-ip": "^0.0.41",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.15.0",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "openapi-typescript": "^7.6.1",
    "prettier": "3.5.3",
    "prisma": "^6.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }

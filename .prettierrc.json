{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "tabWidth": 2,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "semi": false,
  "printWidth": 120,
  "importOrder": [
    "^@/lib",
    "^@/lib/(.*)$",
    "^@/middlewares",
    "^@/middlewares/(.*)$",
    "^@/mocks",
    "^@/mocks/(.*)$",
    "^@/routes",
    "^@/routes/(.*)$",
    "^@/services",
    "^@/services/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
